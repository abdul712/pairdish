  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@cloudflare/vite-plugin": "^1.7.5",
    "@eslint/js": "^9.29.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@tailwindcss/vite": "^4.0.0-alpha.37",
    "@types/node": "^24.0.4",
